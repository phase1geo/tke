filepatterns
{*.sass}

vimsyntax
{sass}

embedded {
  {SCSS {} {}}
}

matchcharsallowed
{paren double single}

tabsallowed
{0}

casesensitive
{0}

icomment {{//}}

lcomments {{//}}

bcomments {{{/\*} {\*/}}}

strings {{"} {'}}

numbers
{
  HighlightRegexp {\m([0-9]+|#[0-9a-fA-F]+|[0-9]+%|[0-9]*\.[0-9]+)} {}
}

punctuation
{
  HighlightRegexp {[][\{\}\(\);:,.<>+/~#*^$&!=%-]} {}
}

miscellaneous1 {
  HighlightWithOnlyCharStart \$ {}
}

miscellaneous2 {
  HighlightWithOnlyCharStart @ {}
}

