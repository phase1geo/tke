filepatterns
{*.json}

matchcharsallowed
{curly square paren double single}

tabsallowed
{0}

casesensitive
{1}

indentation
{{curly}}

icomment {{//}}

comments {{{/\*} {\*/}} {{//} {$}} {# {$}}}

strings {double single}

numbers
{
  HighlightRegexp {\m[0-9]+|[0-9]+\.[0-9e]+|0x[0-9a-fA-F]+|Infinity|NaN} {}
}

punctuation
{
  HighlightRegexp {[][\{\};:,()#=.@&!?<>%|^~+*/-]} {}
}

miscellaneous3 {
  HighlightRegexp {\w+\s*:} {}
}
